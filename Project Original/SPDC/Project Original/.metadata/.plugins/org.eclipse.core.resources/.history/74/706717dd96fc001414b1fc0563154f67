			FileReader fr = new FileReader("C:/inputsms.txt");
			BufferedReader br = new BufferedReader(fr);

			String line = br.readLine();
			while (line != null) {
				String[] parts = line.split(" ");
				/* for (String w : parts) {
					System.out.println(w);
				} */
				System.out.println(parts[0]);
